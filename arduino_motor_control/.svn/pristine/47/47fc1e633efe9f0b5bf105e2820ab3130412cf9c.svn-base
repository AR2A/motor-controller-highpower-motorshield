<!--Start the communication with the IMU Sensor -->
<!--ATTENTION: Before starting this node, make sure, that the port is correct!-->
<launch>  
    <node pkg="rosserial_python" type="serial_node.py" name="ArduinoMotorControl">
     <param name="port" value="/dev/ttyACM1"/> 
     <param name="baud" value="57600"/>
    </node>
</launch>     
